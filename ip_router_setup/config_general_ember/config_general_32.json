{
  "server_port": 9000,
  "ui_port": 5000,
  "fast_routing": false,
  "allow_patching": true,
  "global_video_matrix_target": {
    "instances": "created",
    "path": "rx-end",
    "exclude": []
  },
  "global_video_matrix_source": {
    "instances": "created",
    "path": "tx-end",
    "exclude": []
  },
  "machines": [
    {
        "blade": "http://xxx.xxx.xxx.xxx", // insert here the ip address of the blade master Blade.
        "name": "AT300-1",
        "blade_audio_matrix": [{
            "name": "audio_matrix",
            "outputs": ["SHUFFLER"],
            "inputs": ["RTP", "SDI", "MADI", "SRC"],
            "max_input_channels": 16,
            "max_output_channels":16
        }]
    },
    {
        "blade": "http://XXX.XXX.XXX.XXX", // insert here the ip address of the other blades that you want to use in your setup.
        "name": "AT300-die-andere",
        "blade_audio_matrix": [{
            "name": "audio_matrix",
            "outputs": ["SHUFFLER"],
            "inputs": ["RTP", "SDI", "MADI", "SRC"],
            "max_input_channels": 16,
            "max_output_channels":16
        }]
    }
]
}